{
  "name": "Instagram Yourself!",
  "version": "0.1.0",
  "description": "Extension helps you instagram effectively!",
  "icons": {
    "128": "images/morejustmedia.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "images/morejustmedia.png"
  },
  "permissions": [
    "activeTab",
    "background",
    "webRequest",
    "webRequestBlocking",
    "*://*.instagram.com/",
    "*://*/*"
  ],
  "manifest_version": 2
}
