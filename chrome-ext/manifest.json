{
  "name": "Instagram Yourself!",
  "version": "0.1.4",
  "description": "Extension helps you instagram effectively!",
  "icons": {
    "192": "images/icon.png"
  },
  "background": {
    "scripts": [
      "shared/credentials.js",
      "background/main.js",
      "background/request_history.js",
      "background/insecure_headers.js",
      "background/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": "images/icon.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "background",
    "webRequest",
    "webRequestBlocking",
    "*://*.instagram.com/",
    "file://*/*",
    "https://caffeinum.github.io/*",
    "https://instagrambot.github.io/*"
  ],
  "externally_connectable": {
    "matches": [
      "http://localhost/*",
      "https://caffeinum.github.io/*",
      "https://instagrambot.github.io/*"
    ]
  },
  "manifest_version": 2
}
