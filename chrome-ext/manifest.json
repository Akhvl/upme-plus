  "icons": {
    "192": "images/icon.png"
  },
  "background": {
    "scripts": [
      "shared/credentials.js",
      "background/services/insecure_headers.js",
      "background/services/on_install.js",
      "background/main.js",
      "background/storage/collection.js",
