{
  "name": "Instagram Yourself!",
  "version": "0.1.2",
  "description": "Extension helps you instagram effectively!",
  "icons": {
    "192": "images/icon.png"
  },
  "background": {
    "scripts": [
      "shared/credentials.js",
      "background/main.js",
      "background/request_history.js",
      "background/insecure_headers.js",
      "background/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": "images/icon.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "background",
    "webRequest",
    "webRequestBlocking",
    "*://*.instagram.com/",
    "file://*/*",
    "https://caffeinum.github.io/*",
    "https://instagrambot.github.io/*"
  ],
  "content_scripts": [
   {
     "matches": [
       "file://*/*",
       "https://caffeinum.github.io/*",
       "https://instagrambot.github.io/*"
     ],
     "js": [
       "shared/instagram_connector.js",
       "inject/helpers.js",
       "inject/document_connector.js",
       "inject/buttons.js",
       "inject/main.js"
     ]
   }
 ],
  "manifest_version": 2
}
