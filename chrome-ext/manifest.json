      "http://localhost/*",
      "https://dashboard.gramup.me/*",
      "https://api.gramup.me/*",
      "https://dm.gramup.me/*",
      "https://*.gramup.me/*",
      "https://*.okhlopkov.now.sh/*",
      "https://*.caffeinum.now.sh/*",
      "https://instagrambot.github.io/*"
    ]
